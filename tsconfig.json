{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
     "removeComments": false,
     "preserveConstEnums": false,
     "noImplicitAny": false,
     "types" : ["babylonjs"]
